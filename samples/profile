cpu {				# cpu injection part
profile {			# the load profile itself
0 {				# cpuid of the cpu to load
00:00:00        70		# MANDATORY : time 00:00:00 must be present for each cpu. From the beginning of the injection we load 70% of the cpu.
00:01:00        30		# after one minute we load only 30% the cpu
00:02:00        50
}
1 {				# another cpu loaded
00:00:00        70
}
2 {
00:00:00        70
}
3 {
00:00:00        70
}
}				# end of profile
}				# end of cpu injection part
